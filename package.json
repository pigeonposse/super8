	"scripts": {
		"prepare": "husky install .dev/husky",
		"core": "pnpm --filter core $@",
		"core-react": "pnpm --filter core-react $@",
		"docs": "pnpm --filter docs $@",
		"exts": "pnpm --filter exts $@",
		"app": "pnpm --filter app $@",
		"dev": "turbo dev",
		"dev:core": "turbo dev --filter=core --filter=core-react",
		"dev:exts": "turbo build --filter=core && turbo dev --filter=exts",
		"dev:app": "turbo build --filter=core && turbo dev --filter=app --filter=docs",
		"dev:docs": "turbo build --filter=core && turbo dev --filter=docs",
		"dev-info": "md docs/dev-info.md",
		"build": "turbo build",
		"build:core": "turbo lint build test --filter=core --filter=core-react",
		"build:exts": "turbo lint build test --filter=core --filter=exts",
		"build:app": "turbo lint build test --filter=core --filter=app",
		"build:docs": "turbo lint build test --filter=docs",
		"lint": "pnpm eslint .",
		"lint-fix": "pnpm eslint . --fix",
		"test": "turbo test",
		"cm": "cz",
		"cm-origin": "git add . && pnpm cm && git push -f origin $@",
		"update-version": "changeset && changeset version",
		"push-main": "pnpm cm-origin main",
		"push-dev": "pnpm cm-origin dev",
		"publish-npm": "pnpm build:core && pnpm update-version && changeset publish",
		"todo": "node .dev/todo.mjs"
	},
	"commitlint": {
		"extends": [
			"gitmoji"
		],
		"rules": {
			"header-max-length": [
				0,
				"always",
				100
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".dev/cz-config.cjs"
		}
	},
	"lint-staged": {
		"*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json}": "eslint"
	},
