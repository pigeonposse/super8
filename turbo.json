{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".sveltekit/**"]
      },
      "deploy": {
        "dependsOn": ["build", "test", "lint"]
      },
      "test": {
        "dependsOn": ["build"],
        "inputs": ["tests/**"]
      },
      "lint": {},
      "dev": {
        "cache": false,
        "persistent": true
      }
    }
  }